  <div class="container-fluid">
    <%= form_for @class_session do |f| %>
        <div class="form-group" style="width:30%;">
          <%= f.label :name %>
          <%= f.text_field :name, class: "form-control", autofocus: true, placeholder: "Session Name" %>
        </div>
        <div class="form-group">
          <%= f.submit class: 'btn btn-primary'  %>
        </div>
    <% end %>
  </div>
